<!doctype html>
<html lang="en" xmlns="http://www.w3.org/1999/html">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/public/css/style_lab1.css">
    <title>Document</title>
</head>
<body>
<div class="bg_img">
    <div class="top">
        <a href="/lab1">
            <img class="icon_back" src="public/img/back.png">
        </a>
        <a href="/login">
            <img class="icon_menu" src="public/img/menu.png">
        </a>
    </div>
    <div class="choice">
        <p>

        </p>
        <form method="post" action="/mark">
            <div class="list_1">
                <p class="text_title">Frontend - разработчик</p>
                <table class="table_1">
                    <tr class="table">
                        <td>№</td>
                        <td>Профессиональное качество</td>
                    </tr>
                    <% for(let i=0; i<5; i++) { %>
                        <tr>
                            <td><%= (i+1) %></td>
                            <td class="td_1"><%= FE[i] %></td>
                        </tr>
                    <% } %>
                </table>
            </div>
            <div class="list_button">
                <button class="sent" onclick="">Отправить</button>
            </div>
        </form>
    </div>
</div>
</body>
</html>
